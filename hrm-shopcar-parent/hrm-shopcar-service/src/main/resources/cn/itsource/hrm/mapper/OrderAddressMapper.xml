<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.itsource.hrm.mapper.OrderAddressMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.itsource.hrm.domain.OrderAddress">
        <id column="id" property="id" />
        <result column="createTime" property="createtime" />
        <result column="updateTime" property="updatetime" />
        <result column="orderSn" property="ordersn" />
        <result column="contacts" property="contacts" />
        <result column="address" property="address" />
        <result column="phone" property="phone" />
        <result column="login_id" property="loginId" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, createTime, updateTime, orderSn, contacts, address, phone, login_id
    </sql>

</mapper>
